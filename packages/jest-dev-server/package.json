{
  "name": "jest-dev-server",
  "description": "Starts a server before your Jest tests and tears it down after.",
  "version": "3.4.0",
  "main": "index.js",
  "repository": "https://github.com/smooth-code/jest-puppeteer/tree/master/packages/jest-dev-server",
  "author": "Greg Berg√© <berge.greg@gmail.com>",
  "license": "MIT",
  "keywords": [
    "jest",
    "jest-environment",
    "server"
  ],
  "scripts": {
    "prebuild": "rm -rf lib/",
    "build": "babel --config-file ../../babel.config.js -d lib --ignore \"**/*.test.js\" src",
    "dev": "yarn build --watch",
    "prepublishOnly": "yarn build",
    "snyk-protect": "snyk protect",
    "prepublish": "npm run snyk-protect"
  },
  "dependencies": {
    "chalk": "^2.4.1",
    "cwd": "^0.10.0",
    "find-process": "^1.1.3",
    "inquirer": "^6.2.0",
    "spawnd": "^3.4.0",
    "terminate": "^2.1.0",
    "wait-port": "^0.2.2",
    "snyk": "^1.258.2"
  },
  "snyk": true
}
